<% layout('layouts/boilerplate')%>
<h1>All Contacts</h1>
    <a href="/contacts/new">New Contact</a>
    <table>
        <tr>
            <th>Name</th>
            <th>Title</th>
            <th>Program</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
        <% for(person of people) { %> 
        <tr>
            <td><%=person.name%></td>
            <td><%=person.title%></td>
            <td><%=person.program%></td>
            <td><%=person.email%></td>
            <td><%=person.phoneNumber%></td>
        </tr>
        <% } %> 
    </table>